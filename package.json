{
  "name": "@danehansen/sprite",
  "version": "0.2.1",
  "description": "sprite",
  "main": "danehansen-Sprite.min.js",
  "scripts": {
    "max": "webpack",
    "test": "mocha --require babel-register",
    "test-watch": "mocha --require babel-register -w",
    "coverage": "nyc mocha  --require babel-register",
    "coverage-report": "nyc --reporter=html mocha  --require babel-register",
    "git": "git add -A && git commit -m 'squish me' & git push -f origin master",
    "min": "webpack -p",
    "pub": "npm run test && npm run min && sleep 1 && npm run git && npm publish --access public",
    "prepublish": "npm run max"
  },
  "keywords": [
    "animation",
    "gsap",
    "GreenSock",
    "sprite",
    "spritesheet",
    "timeline",
    "TweenLite"
  ],
  "author": "Dane Hansen <dane@danehansen.com> (http://www.danehansen.com/)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "jsdom": "^9.11.0",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.3.0",
    "nyc": "^10.3.0",
    "sinon": "^2.2.0",
    "webpack": "^2.5.0"
  },
  "dependencies": {
    "@danehansen/event-dispatcher": "latest",
    "@danehansen/math": "latest",
    "gsap": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danehansen/Sprite"
  }
}
